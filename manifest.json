{
  "manifest_version": 2,
  "name": "Themify",
  "version": "2.52",
  "description": "View the colors of a website",
  "short_name": "View the colors of a website",
  "permissions": ["activeTab", "storage"],
  "browser_action": {
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    },
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "default": "Alt+F",
        "mac": "Alt+F"
      }
    },
    "eye-dropper": {
      "suggested_key": {
        "default": "Alt+D",
        "mac": "Alt+D"
      },
      "description": "Opens eyedropper"
    }
  }
}
