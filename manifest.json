{
  "manifest_version": 2,
  "name": "Themify",
  "version": "2.3",
  "description": "View the colors of a website",
  "short_name": "iew the colors of a website",
  "permissions": ["activeTab", "storage"],
  "browser_action": {
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    },
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}
